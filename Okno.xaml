<Window x:Class="KalendfrNow.Okno"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KalendfrNow"
        mc:Ignorable="d"
        Title="Okno" Height="450" Width="800">
    <Grid Margin="0,10,10,0">

        <Grid>
            <ListBox x:Name="listic" HorizontalAlignment="Left" Height="324" VerticalAlignment="Top" Width="800" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,10,0,0"/>
            <Button x:Name="save" Content="Сохранить" HorizontalAlignment="Center" Height="40" Margin="0,359,0,0" VerticalAlignment="Top" Width="250" FontSize="16" Click="SaveClick"/>

        </Grid>

    </Grid>
</Window>
